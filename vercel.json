{
  "installCommand": "npm install",
  "buildCommand": "npm run build",
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ],
  "redirects": [
    {
      "source": "/catalog/:id",
      "destination": "/homes-for-sale/:id",
      "permanent": true
    },
    {
      "source": "/catalog",
      "destination": "/homes-for-sale",
      "permanent": true
    },
    {
      "source": "/single-wide",
      "destination": "/single-wide-mobile-homes",
      "permanent": true
    },
    {
      "source": "/double-wide/:id",
      "destination": "/double-wide-mobile-homes/:id",
      "permanent": true
    },
    {
      "source": "/double-wide",
      "destination": "/double-wide-mobile-homes",
      "permanent": true
    },
    {
      "source": "/modular-homes/:id",
      "destination": "/modular-homes-for-sale/:id",
      "permanent": true
    },
    {
      "source": "/modular-homes",
      "destination": "/modular-homes-for-sale",
      "permanent": true
    },
    {
      "source": "/manufacturers",
      "destination": "/manufactured-home-manufacturers",
      "permanent": true
    },
    {
      "source": "/deals",
      "destination": "/mobile-home-deals",
      "permanent": true
    },
    {
      "source": "/financing",
      "destination": "/mobile-home-financing",
      "permanent": true
    },
    {
      "source": "/land-home",
      "destination": "/land-and-home-packages",
      "permanent": true
    },
    {
      "source": "/la-restore",
      "destination": "/la-restore-grants",
      "permanent": true
    },
    {
      "source": "/insurance",
      "destination": "/mobile-home-insurance",
      "permanent": true
    },
    {
      "source": "/services",
      "destination": "/warranty-service-department",
      "permanent": true
    },
    {
      "source": "/how-it-works",
      "destination": "/buying-process",
      "permanent": true
    },
    {
      "source": "/parts",
      "destination": "/mobile-home-parts-store",
      "permanent": true
    },
    {
      "source": "/about",
      "destination": "/about-gulf-south-homes",
      "permanent": true
    },
    {
      "source": "/contact",
      "destination": "/contact-gulf-south-homes",
      "permanent": true
    },
    {
      "source": "/:path+/",
      "destination": "/:path+",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*\\.(js|css|json|xml|txt|svg|ico|png|jpg|jpeg|webp|woff|woff2))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(html))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Strict-Transport-Security",
          "value": "max-age=63072000; includeSubDomains; preload"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "SAMEORIGIN"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/:path*.mp4",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/mp4"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        }
      ]
    },
    {
      "source": "/assets/video/(.*)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "video/mp4"
        },
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        },
        {
          "key": "Accept-Ranges",
          "value": "bytes"
        }
      ]
    }
  ]
}

